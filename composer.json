{
    "name": "shooglypeg/valueobjects",
    "description": "Shared kernel value objects for Shoogly projects",
    "keywords": [],
    "homepage": "https://github.com/pedros80/ShooglyPeg-ValueObjects",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Somerville",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "beberlei/assert": "3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ShooglyPeg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
